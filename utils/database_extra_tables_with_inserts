#
# TABLE STRUCTURE FOR: Product_Special
#

DROP TABLE IF EXISTS `Product_Special`;

CREATE TABLE `Product_Special`
(
  `promo_code`    int(10) unsigned NOT NULL,
  `product_id`    int(10) unsigned NOT NULL,
  `start_date`    timestamp        NOT NULL DEFAULT current_timestamp(),
  `end_date`      timestamp        NULL     DEFAULT NULL,
  `special_price` decimal(10, 0)   NOT NULL,
  PRIMARY KEY (`promo_code`),
  KEY `product_special__product_fk` (`product_id`),
  CONSTRAINT `product_special__product_fk` FOREIGN KEY (`product_id`) REFERENCES `Product` (`id`) ON DELETE CASCADE
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8
  COLLATE = utf8_unicode_ci;

