#
# TABLE STRUCTURE FOR: Basket_Product
#

DROP TABLE IF EXISTS `Basket_Product`;

CREATE TABLE `Basket_Product`
(
  `quantity`   smallint(5) unsigned NOT NULL,
  `basket_id`  int(10) unsigned     NOT NULL,
  `product_id` int(10) unsigned     NOT NULL,
  KEY `basket_product__basket_fk` (`basket_id`),
  KEY `basket_product__product_fk` (`product_id`),
  CONSTRAINT `basket_product__basket_fk` FOREIGN KEY (`basket_id`) REFERENCES `Basket` (`id`) ON DELETE CASCADE,
  CONSTRAINT `basket_product__product_fk` FOREIGN KEY (`product_id`) REFERENCES `Product` (`id`) ON DELETE CASCADE
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8
  COLLATE = utf8_unicode_ci;

