#
# TABLE STRUCTURE FOR: Order
#

DROP TABLE IF EXISTS `Order`;

CREATE TABLE `Order`
(
  `id`           int(10) unsigned                                                            NOT NULL AUTO_INCREMENT,
  `customer_id`  int(10) unsigned                                                            NOT NULL,
  `date_created` timestamp                                                                   NOT NULL DEFAULT current_timestamp(),
  `order_status` enum ('new','hold','shipping','delivered','closed') COLLATE utf8_unicode_ci NOT NULL DEFAULT 'new',
  `phone_number` varchar(15) COLLATE utf8_unicode_ci                                         NOT NULL,
  PRIMARY KEY (`id`),
  KEY `order__customer_fk` (`customer_id`),
  CONSTRAINT `order__customer_fk` FOREIGN KEY (`customer_id`) REFERENCES `Customer` (`id`) ON DELETE CASCADE
) ENGINE = InnoDB
  AUTO_INCREMENT = 101
  DEFAULT CHARSET = utf8
  COLLATE = utf8_unicode_ci;

