#
# TABLE STRUCTURE FOR: Product_Stock
#

DROP TABLE IF EXISTS `Product_Stock`;

CREATE TABLE `Product_Stock`
(
  `quantity`   smallint(5) unsigned NOT NULL,
  `stock_id`   tinyint(3) unsigned  NOT NULL,
  `product_id` int(10) unsigned     NOT NULL,
  KEY `product_stock__stock_fk` (`stock_id`),
  KEY `product_stock__product_fk` (`product_id`),
  CONSTRAINT `product_stock__product_fk` FOREIGN KEY (`product_id`) REFERENCES `Product` (`id`) ON DELETE CASCADE ON UPDATE CASCADE,
  CONSTRAINT `product_stock__stock_fk` FOREIGN KEY (`stock_id`) REFERENCES `Stock` (`id`) ON DELETE CASCADE ON UPDATE CASCADE
) ENGINE = InnoDB
  DEFAULT CHARSET = utf8
  COLLATE = utf8_unicode_ci;

