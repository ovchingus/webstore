#
# TABLE STRUCTURE FOR: Customer
#

DROP TABLE IF EXISTS `Customer`;

CREATE TABLE `Customer`
(
  `id`         int(10) unsigned                     NOT NULL AUTO_INCREMENT,
  `first_name` varchar(31) COLLATE utf8_unicode_ci  NOT NULL,
  `last_name`  varchar(31) COLLATE utf8_unicode_ci  NOT NULL,
  `email`      varchar(127) COLLATE utf8_unicode_ci NOT NULL,
  `password`   char(20) COLLATE utf8_unicode_ci     NOT NULL,
  PRIMARY KEY (`id`),
  UNIQUE KEY `customer_email` (`email`)
) ENGINE = InnoDB
  AUTO_INCREMENT = 16
  DEFAULT CHARSET = utf8
  COLLATE = utf8_unicode_ci;

